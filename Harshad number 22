#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
 
int main(){

  int n,sum=0;
  scanf("%d",&n);
  int temp=n;
  while(n>0)
  {
      sum+=n%10;
      n/=10;
  }
  if(temp%sum==0)
    {
    printf("Harshad Number");
    }
  else 
    printf("Not Harshad Number");

return 0;
}
